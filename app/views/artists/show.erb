<h2><%=@artist.name%></h2>
<br></br>

<h3>Songs:</h3>

<br></br>

<%@artist.songs.each do |song|%>
  <li>
    <a href="/songs/<%=song.slug%>"><%= song.name %></a>
  </li>
  <% end %>

<br></br>

<h3>Genres:</h3>

<br></br>

<%@artist.genres.each do |genre|%>
  <li>
  <%= genre.name %>
      <a href="/genres/<%=genre.slug%>"> </a>
  </li>
<% end %>

<br></br>
